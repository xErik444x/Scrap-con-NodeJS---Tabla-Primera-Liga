{
   "version": 2,
   "builds": [{
    "src": "app.js",
    "use": "@now/node-server",
    "config": { "maxLambdaSize": "50mb" }
  },{
    "src": "./public/*/**",
    "use": "@now/static"
  }],
  "routes": [{
    "src": "/dev/(css|js)/(.*)",
    "dest": "/public/($1)/($2)"
  },{
    "src": "/",
    "dest": "app.js"
  }]
}
